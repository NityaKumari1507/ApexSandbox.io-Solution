public Integer nextPrime(Integer num) {
    //code here
    if(num<=0){
        retun 2;
    }
    for(integer i=num+1;;i++){
         integer flag=0;
        for(integer j=2;j<=math.sqrt(i);j++){
           if(math.mod(i,j)==0) {
               flag=1;
               break; 
            }  
        }
         if(flag==0)
         return i;
    }
    return 0;
   
}