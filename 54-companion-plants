public Boolean companionPlants(String plant1, String plant2) {
    //code here
    Map<string,string> m=new map<string,string>{'onions'=>'carrots','lettuce'=>'cucumbers','lettuce'=>'onions','onions'=>'tomatoes','tomatoes'=>'onions','onions'=>'lettuce','carrots'=>'onions','cucumbers'=>'lettuce'};
    return (m.get(plant1)==plant2)?true:false;
}